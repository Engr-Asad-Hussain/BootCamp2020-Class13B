sAvatars[object].img} onClick={\n                                ()=>{\n                                    setValues(object);\n                                    Animations(index);\n                                }\n                            }/>\n                        </div>\n                    );\n                })}\n            </div>\n            <div className=\"messages\">\n                <p><b>{itemsAvatars[values].name}</b></p><br/><br/>\n                <p>{itemsAvatars[values].intro}</p>\n            </div>\n            <div className=\"foot-avatar\">\n                <Button className={classes.root} variant=\"contained\">\n                    Keep Chatting!\n                </Button>\n                <Button className={classes.root} variant=\"contained\">\n                    Get in Touch!\n                </Button>\n            </div>\n        </div>\n    );\n}\nexport default Avatars;","/home/asad/Cloud-Computing/BootCamp2020/Class13-z-Project4B/example-01/src/index.js",[],"/home/asad/Cloud-Computing/BootCamp2020/Class13-z-Project4B/example-01/src/components/MainPage/Logo.js",["60","61","62"],"/home/asad/Cloud-Computing/BootCamp2020/Class13-z-Project4B/example-01/src/components/Layers/UXDesign.js",["63","64"],"/home/asad/Cloud-Computing/BootCamp2020/Class13-z-Project4B/example-01/src/components/SideBar.js",[],{"ruleId":"65","severity":1,"message":"66","line":1,"column":8,"nodeType":"67","messageId":"68","endLine":1,"endColumn":13},{"ruleId":"65","severity":1,"message":"69","line":6,"column":11,"nodeType":"67","messageId":"68","endLine":6,"endColumn":20},{"ruleId":"70","severity":1,"message":"71","line":43,"column":17,"nodeType":"72","endLine":43,"endColumn":45},{"ruleId":"65","severity":1,"message":"69","line":6,"column":11,"nodeType":"67","messageId":"68","endLine":6,"endColumn":20},{"ruleId":"70","severity":1,"message":"71","line":22,"column":17,"nodeType":"72","endLine":22,"endColumn":45},{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","rep